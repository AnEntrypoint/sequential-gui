{
  "name": "admin-gui",
  "version": "1.0.0",
  "description": "Admin GUI for sequential-ecosystem task management",
  "private": true,
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=server\" \"npm run dev --workspace=web\"",
    "build": "npm run build --workspaces",
    "start": "npm start --workspace=server",
    "test": "npm test --workspaces"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
