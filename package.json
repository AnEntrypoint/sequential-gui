{
  "name": "sequential-gui",
  "version": "1.0.0",
  "description": "Admin GUI for sequential-ecosystem task management",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "bin": {
    "sequential-gui": "./gui-launcher.js"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=server\" \"npm run dev --workspace=web\"",
    "build": "npm run build --workspaces",
    "start": "node gui-launcher.js",
    "test": "npm test --workspaces"
  },
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
